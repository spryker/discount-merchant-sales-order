{
  "name": "spryker/discount-merchant-sales-order",
  "type": "library",
  "description": "DiscountMerchantSalesOrder module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/discount": "^9.10.0",
    "spryker/kernel": "^3.33.0",
    "spryker/merchant-sales-order": "^0.7.3",
    "spryker/merchant-sales-order-extension": "^0.1.2"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
